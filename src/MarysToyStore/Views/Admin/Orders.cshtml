@model IEnumerable<Order>
<div>
	<table class="table">
		<tr>
			<th>ID</th>
			<th>Date Ordered</th>
			<th>Order Status</th>
			<th></th>
		</tr>
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Id</td>
				<td>@item.DateOrdered</td>
				<td>@item.OrderStatus</td>
				<td>
					<a asp-controller="Admin" asp-action="OrderDetails" asp-route-id="@item.Id">Details</a>
				</td>
			</tr>
		}
	</table>
</div>
